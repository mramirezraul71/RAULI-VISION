{
  "version": 2,
  "builds": [
    {
      "src": "dashboard/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dashboard/$1"
    }
  ],
  "functions": {
    "api/espejo/index.go": {
      "runtime": "go1.x",
      "memory": 512
    },
    "api/proxy/index.go": {
      "runtime": "go1.x",
      "memory": 512
    }
  }
}