{"version": 2, "builds": [{"src": "dashboard/package.json", "use": "@vercel/static-build", "config": {"distDir": "dist"}}], "routes": [{"src": "/api/(.*)", "dest": "https://your-backend-url.onrender.com/api/$1", "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]}, {"src": "/auth/(.*)", "dest": "https://your-backend-url.onrender.com/auth/$1", "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]}, {"src": "/(.*)", "dest": "/dashboard/$1"}], "env": {"VITE_API_URL": "https://your-backend-url.onrender.com"}, "headers": [{"source": "/api/(.*)", "headers": [{"key": "Access-Control-Allow-Origin", "value": "*"}, {"key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE", "OPTIONS"}, {"key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization"}]}, {"source": "/static/(.*)", "headers": [{"key": "Cache-Control", "value": "public, max-age=31536000, immutable"}]}]}
